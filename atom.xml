---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Noah Boyle Blog</title>
 <link href="http://noahboyle.github.io/jekyll/atom.xml" rel="self"/>
 <link href="http://noahboyle.github.io/jekyll/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://noahboyle.github.io/jekyll/</id>
 <author>
   <name>Noah Boyle</name>
   <email>nboyle@mica.edu</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://noahboyle.github.io/jekyll{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://noahboyle.github.io/jekyll{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>